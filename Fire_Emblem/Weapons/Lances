public class bLance {
  int mt;
  int hit;
  int avo;
  int crit;
  int critavo;
  int range;
  int itemnum;
  int x2;
  String name;
  
  bLance() {
    mt = 5;
    hit = 90;
    critavo = 10;
    range = 1;
    itemnum = 11;
    name = "Bronze Lance";
  }
}

